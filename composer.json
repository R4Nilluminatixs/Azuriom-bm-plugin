{
    "name": "azuriom/battlemetrics",
    "type": "azuriom-plugin",
    "description": "Plugin to integrate Battlemetrics into Azuriom",
    "autoload": {
        "psr-4": {
            "Azuriom\\Plugin\\Battlemetrics\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1"
    },
    "replace": {
        "guzzlehttp/guzzle": "*",
        "guzzlehttp/promises": "*",
        "guzzlehttp/psr7": "*",
        "monolog/monolog": "*",
        "nesbot/carbon": "*",
        "psr/container": "*",
        "psr/event-dispatcher": "*",
        "psr/http-client": "*",
        "psr/http-factory": "*",
        "psr/http-message": "*",
        "psr/log": "*",
        "psr/simple-cache": "*"
    }
}
